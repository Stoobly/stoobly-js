{
  "name": "stoobly",
  "version": "0.5.3",
  "description": "Javacript library for stoobly-agent",
  "files": [
    "dist/**"
  ],
  "main": "dist/cjs/stoobly.js",
  "module": "dist/esm/stoobly.js",
  "types": "dist/types/stoobly.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/stoobly.js",
      "require": "./dist/cjs/stoobly.js",
      "types": "./dist/types/stoobly.d.ts"
    },
    "./constants": {
      "import": "./dist/esm/constants.js",
      "require": "./dist/cjs/constants.js",
      "types": "./dist/types/constants.d.ts"
    },
    "./types": {
      "import": "./dist/esm/types.js",
      "require": "./dist/cjs/types.js",
      "types": "./dist/types/types.d.ts"
    }
  },
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "clean": "rm -rf ./node_modules/.cache ./dist",
    "lint": "eslint --fix --ext .js,.jsx,.ts ./src ./test",
    "prepack": "npm run build && npm ci --omit dev",
    "test": "jest",
    "test:e2e": "npm run build && npm run test:cypress && npm run test:playwright",
    "test:cypress": "start-server-and-test test-server http://localhost:3000 test:cypress:run",
    "test:cypress:open": "cypress open --config-file e2e/cypress.config.mjs",
    "test:cypress:run": "cypress run --config-file e2e/cypress.config.mjs",
    "test:playwright": "start-server-and-test test-server http://localhost:3000 test:playwright:run",
    "test:playwright:run": "playwright test --config=e2e/playwright.config.ts",
    "test-server": "node e2e/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stoobly/stoobly-js.git"
  },
  "keywords": [
    "api",
    "client",
    "npm",
    "stoobly"
  ],
  "author": "themathewle@gmail.com",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Stoobly/stoobly-js/issues"
  },
  "homepage": "https://github.com/Stoobly/stoobly-js#readme",
  "dependencies": {
    "axios": "^1.13.1",
    "qs": "^6.11.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.2",
    "@babel/preset-typescript": "^7.21.5",
    "@cypress/webpack-preprocessor": "^7.0.2",
    "@playwright/test": "^1.56.1",
    "@rollup/plugin-alias": "^6.0.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/jest": "^29.5.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.11",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "cypress": "^14.5.3",
    "cypress-terminal-report": "^7.3.3",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^5.1.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-mock-axios": "^4.7.2",
    "rollup": "^4.53.3",
    "rollup-plugin-dts": "^6.3.0",
    "start-server-and-test": "^2.0.13",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.4",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.8.2",
    "util": "^0.12.5"
  }
}
